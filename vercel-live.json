{
  "name": "prediction-venue-live",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "REACT_APP_API_URL": "https://prediction-venue-live.vercel.app",
    "REACT_APP_VENUE_NAME": "Ether's Live Prediction Exchange",
    "REACT_APP_ENVIRONMENT": "production",
    "REACT_APP_DATA_SOURCE": "live",
    "REACT_APP_ENABLE_LIVE_DATA": "true"
  },
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install && cd ../api && pip install -r requirements.txt"
}